  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/client-sqs": "^3.279.0",
    "amqplib": "^0.10.2",
    "axios": "^1.3.4",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase": "^9.17.2",
    "firebase-admin": "^11.5.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.19",
    "redis": "^4.6.5",
    "uuidv4": "^6.2.13",
